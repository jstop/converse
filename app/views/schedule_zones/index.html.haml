%h1 Listing schedule_zones

%table
  %thead
    %tr
      %th Payload
      %th
      %th
      %th

  %tbody
    - @schedule_zones.each do |schedule_zone|
      %tr
        %td= schedule_zone.payload
        %td= link_to 'Show', schedule_zone
        %td= link_to 'Edit', edit_schedule_zone_path(schedule_zone)
        %td= link_to 'Destroy', schedule_zone, method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Schedule zone', new_schedule_zone_path
